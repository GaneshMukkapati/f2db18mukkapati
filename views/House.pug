extends layout

block content
  h1= title
  - var result = results 
  div.container-fluid 
    div.row 
        each item, index in result
            div.col-sm-2(onclick=`buy(${index})`) House Name: #{item["House_Name"]} <br> Housing Company: #{item["Housing_Company"]} <br> House Size: #{item["House_Size"]} <br> Housing Company Rating: #{item["Housing_Company_Rating"]}
  script.
    function buy(index) {
        alert("You purchased item " + index);
    }